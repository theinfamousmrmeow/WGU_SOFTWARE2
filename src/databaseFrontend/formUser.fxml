<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="205.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="databaseFrontend.controllerUser">
   <children>
      <ButtonBar layoutX="64.0" layoutY="143.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="buttonSave" mnemonicParsing="false" onAction="#addUser" text="Save" />
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#backToMain" text="Cancel" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="tfUsername" layoutX="114.0" layoutY="44.0" />
      <TextField fx:id="tfPassword" layoutX="114.0" layoutY="97.0" />
      <Label layoutX="32.0" layoutY="47.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="100.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
